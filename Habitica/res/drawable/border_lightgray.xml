<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="1dp"
        android:left="-2dp"
        android:right="-2dp"
        android:top="-2dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="#ff959595" />

            <solid android:color="#00FFFFFF" />
            <padding
                android:bottom="8dp"
                android:left="0dp"
                android:right="0dp"
                android:top="8dp" />
        </shape>
    </item>

</layer-list>